    <edge id="5" source="0" target="1">
        <data key="source_policy">--local_amt=100000</data>
        <data key="target_policy">--base_fee_msat=2200 --fee_rate_ppm=13 --time_lock_delta=20</data>
    </edge>
    <edge id="6" source="1" target="2">
        <data key="source_policy">--local_amt=100000 --push_amt=50000 --base_fee_msat=5500 --fee_rate_ppm=3</data>
    </edge>
